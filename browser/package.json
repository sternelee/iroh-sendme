{
  "name": "sendme-browser",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:wasm": "cd ../browser-lib && cargo build --target=wasm32-unknown-unknown && wasm-bindgen ./target/wasm32-unknown-unknown/debug/sendme_browser.wasm --out-dir=./browser/public/wasm --weak-refs --target=web --debug",
    "build:wasm:release": "cd ../browser-lib && cargo build --target=wasm32-unknown-unknown --release && wasm-bindgen ./target/wasm32-unknown-unknown/release/sendme_browser.wasm --out-dir=./browser/public/wasm --weak-refs --target=web",
    "serve": "servor public"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.562.0",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.6.0-alpha.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "servor": "^4.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vue-tsc": "^3.2.2"
  }
}
